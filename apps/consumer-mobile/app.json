{
  "expo": {
    "name": "CampoTech",
    "slug": "campotech-consumer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "campotech",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#059669"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "tech.campo.consumer",
      "buildNumber": "1",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "CampoTech usa tu ubicación para mostrarte proveedores de servicios cerca tuyo."
      },
      "associatedDomains": [
        "applinks:campotech.com.ar",
        "applinks:www.campotech.com.ar"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#059669"
      },
      "package": "tech.campo.consumer",
      "versionCode": 1,
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "campotech.com.ar",
              "pathPrefix": "/provider"
            },
            {
              "scheme": "https",
              "host": "campotech.com.ar",
              "pathPrefix": "/rate"
            },
            {
              "scheme": "https",
              "host": "www.campotech.com.ar",
              "pathPrefix": "/provider"
            },
            {
              "scheme": "https",
              "host": "www.campotech.com.ar",
              "pathPrefix": "/rate"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "CampoTech usa tu ubicación para mostrarte proveedores de servicios cerca tuyo."
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "consumer-app-project-id"
      }
    }
  }
}
