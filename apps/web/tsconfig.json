{
  "compilerOptions": {
    "target": "ES2020",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/components/*": [
        "./components/*"
      ],
      "@/lib/*": [
        "./lib/*"
      ],
      "@/hooks/*": [
        "./hooks/*"
      ],
      "@/types/*": [
        "./types/*"
      ],
      "@/styles/*": [
        "./styles/*"
      ],
      "@/services/*": [
        "../../src/services/*"
      ],
      "@/src/*": [
        "../../src/*"
      ],
      "pusher": [
        "./node_modules/pusher",
        "../../node_modules/pusher"
      ],
      "ioredis": [
        "./node_modules/ioredis",
        "../../node_modules/ioredis"
      ],
      "bullmq": [
        "./node_modules/bullmq",
        "../../node_modules/bullmq"
      ],
      "openai": [
        "./node_modules/openai"
      ],
      "openai/*": [
        "./node_modules/openai/*"
      ]
    },
    "typeRoots": [
      "./types",
      "./node_modules/@types",
      "../../node_modules/@types"
    ],
    "types": [
      "node",
      "react",
      "jest"
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "build",
    "__tests__",
    "**/__tests__/**",
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/*.spec.ts",
    "**/*.spec.tsx",
    "*.config.js",
    "*.config.ts",
    "e2e"
  ]
}