{
  "name": "@campotech/sdk",
  "version": "1.0.0",
  "description": "Official TypeScript/JavaScript SDK for the CampoTech API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "campotech",
    "api",
    "sdk",
    "field-service",
    "fsm",
    "typescript"
  ],
  "author": "CampoTech <api@campotech.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/campotech/sdk-typescript.git"
  },
  "homepage": "https://developers.campotech.com",
  "bugs": {
    "url": "https://github.com/campotech/sdk-typescript/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0"
  }
}
